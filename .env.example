# Google OAuth2 Credentials
GOOGLE_OAUTH_CLIENT_ID=your-client-id-here
GOOGLE_OAUTH_CLIENT_SECRET=your-client-secret-here

# Application Configuration
# PORT is optional, defaults to value in configs/routine.toml [app].port
PORT=8080

# Configuration File Path
CONFIG_FILE=configs/routine.toml

# Note: Application URLs are now configured in configs/routine.toml under the [app] section:
# - app_url: Internal application URL used for OAuth callbacks and general routes
# - public_url: Public URL used for webhooks and external integrations